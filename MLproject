name: iris-classification

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      lakefs_branch: {type: string, default: "main"}
      max_depth: {type: int, default: 5}
      n_estimators: {type: int, default: 100}
    command: "python train.py --lakefs_branch {lakefs_branch} --max_depth {max_depth} --n_estimators {n_estimators}"
  
  preprocess:
    parameters:
      lakefs_branch: {type: string, default: "main"}
    command: "python preprocess.py --lakefs_branch {lakefs_branch}"
